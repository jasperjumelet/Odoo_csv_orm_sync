<?xml version="1.0"?>
<odoo>
<record model="ir.actions.server" id="devices_to_csv_action">
    <field name="name">devices_to_csv</field>
    <field name="model_id" ref="model_device_model"/>
    <field name="state">code</field>
    <field name="code">
        model.to_csv()
    </field>
</record>
<record model="ir.actions.server" id="content_to_csv_action">
    <field name="name">content_to_csv</field>
    <field name="model_id" ref="model_content_model"/>
    <field name="state">code</field>
    <field name="code">
        model.to_csv()
    </field>
</record>
</odoo>
